public class findDuplicate {

    public static void main(String[] args) {
    
    // number are present in between 1 to n-1..
    
    
        int[] arr = {1,2,3,4,3};
        int ans = 0;

        for (int i = 0; i < arr.length; i++) {
            ans = ans ^ arr[i];
        }

        for (int i = 0; i < arr.length; i++) {
            ans = ans ^ i;
        }

        System.out.println(ans);

    }
}
