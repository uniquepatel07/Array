

class practics {

    public static void main(String[] args) {

        // find the unique element present in an array

        int arr[] = {1,2,3,5,3,2,1};

        int ans = 0;
        for (int i = 0; i < arr.length; i++) {
            ans = ans ^ arr[i];

        }
        System.out.println("The unique element is " + ans);

    }
}
